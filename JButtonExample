import javax.swing;
public class JButtonExample{
    public static void main(String[] abc){
        JFrame f = new Frame("JButtonExample");
        Button b = new Button("click Here");
        b.setBounds(150,100,80,20);
        f.add(b);
        f.setsize(400,400);
        f.setLayout(null);
        f.setVisible(true);
    }
public static void main(String[] abc){
    new JButtonExample();
}

}